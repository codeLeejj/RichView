<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="MyToolbar">
        <attr name="left_text" format="string" />
        <attr name="title" format="string" />
        <attr name="title_size" format="dimension" />
        <attr name="title_color" format="color" />
        <attr name="right_icon" format="integer" />
        <attr name="right_text" format="string" />
        <attr name="bottom_line" format="boolean" />
    </declare-styleable>

    <declare-styleable name="StatusBarPlaceView">
        <attr name="backgroundColor" format="color" />
    </declare-styleable>
    <declare-styleable name="Banner">
        <attr name="indicator" format="string" />
        <eat-comment />

        <attr name="autoPlay" format="boolean" />
        <attr name="loop" format="boolean" />
        <attr name="interval" format="integer" />
        <attr name="duration" format="integer" />
        <attr name="indicatorXRatio" format="float" />
        <attr name="indicatorYRatio" format="float" />
        <attr name="colorDefault" format="color" />
        <attr name="colorSelected" format="color" />
        <attr name="drawableDefault" format="reference" />
        <attr name="drawableSelected" format="reference" />
    </declare-styleable>
</resources>